NODE_ENV=development
HOST=0.0.0.0
PORT=3000
LOG_LEVEL=info
# Must include an explicit local DB role (do not rely on implicit OS-user defaults).
DATABASE_URL=postgresql://your_local_db_user@localhost:5432/altcontext_dev
IP_HASH_PEPPER=replace-with-32-char-min-secret
RATE_LIMIT_MAX=120
RATE_LIMIT_TIME_WINDOW=1 minute
SESSION_INACTIVITY_MINUTES=30
HEURISTIC_LINK_WINDOW_MINUTES=15
ENABLE_HEURISTIC_LINKING=true
CORS_ALLOWED_ORIGINS=https://localhost:8080,https://www.altcontext.com
ADMIN_API_KEY=replace-with-24-char-min-secret
SESSION_SECRET=0123456789abcdef0123456789abcdef0123456789abcdef0123456789abcdef
BOOTSTRAP_TENANT_ID=00000000-0000-4000-a000-000000000001
BOOTSTRAP_USER_EMAIL=owner@example.com
BOOTSTRAP_USER_PASSWORD=replace-with-strong-password
ROLLUP_BATCH_DAYS=7
ROLLUP_DEFAULT_PROPERTY_ID=default
METRICS_SUMMARY_CACHE_TTL_MS=15000
METRICS_SUMMARY_CACHE_MAX_ENTRIES=128
METRICS_USE_MATERIALIZED_VIEW=false
METRICS_MATERIALIZED_VIEW_NAME=metrics_daily_summary_mv
PRIVACY_CONTACT_EMAIL=privacy@altcontext.local
